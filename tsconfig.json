{
	"compilerOptions": {
        "rootDir": "./src",
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "es6",
		"target": "es5",
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node"
	},
	"exclude": [
		"node_modules",
		"dist",
		"webpack.config.*.js",
		"webpack.config.js",
		"eslint.config.mjs"
	]
}
